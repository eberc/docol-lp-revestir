{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended"
  ],
  "rules": {
    "linebreak-style": [
      0,
      "error",
      "windows"
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}